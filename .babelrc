{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 0.25%, not dead",
      }
    ]
  ],
  "exclude": [
    "node_modules/**"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-jsx",
    ["@babel/plugin-transform-react-jsx",
    {
      "pragma": "MotorCortex.utils.createDOMElement"
    }]
  ]
}